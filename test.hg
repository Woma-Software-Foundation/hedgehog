func fibonacci(n) {
    if n < 0 {
        return -1;
    } elsif n == 0 {
        return 0;
    } else {
        a = 0;
        b = 1;
        func fn() {
            t = a;
            a = b;
            b = t+b;
        }
        for j=1;j<n;j=j+1 {fn();}
        return b;
    }
}

print(fibonacci(20));

func sum(n) {
	if n==1 {return 1;}
	return sum(n-1)+n;
}
print(sum(5));

