hello()

func fbi(n) {
    a, b = 0, 1
    for a<n {
        print(a)
        a, b = b, a+b
    }
}
print("fbi(100)=", fbi(100))

func factorial(n) {
    if n==0 {return 1}
    return n*factorial(n-1)
}
print("fact(5)", factorial(5))
