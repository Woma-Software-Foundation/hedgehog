# functions
func hello() {
    puts('Hello, world!')
}
hello()

func fbi(n) {
    a, b = 0, 1
    for a < n {
        a, b = b, a+b
    }
    return a
}
puts('fbi(100) =', fbi(100))

func factorial(n) {
    if n==0 {
        return 1
    }
    return n * factorial(n-1)
}
puts('fact(5) =', factorial(5))
