func fibonacci(n) {
    if n < 0 {
        return -1
    } else if n == 0 {
        return 0
    } else {
        a, b = 0, 1
        for j=1;j<n;j=j+1 {
            a, b = b, a+b
        }
        return b
    }
}

puts(fibonacci(20))

func fibonacci(n) {
    if n<0 {return -1}
    else if n==0 {return 0}
    else if n==1 {return 1}
    return fibonacci(n-1)+fibonacci(n-2)
}

puts(fibonacci(20))

