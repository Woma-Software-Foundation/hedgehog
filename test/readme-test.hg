func fbi(n) {
    a, b = 0, 1;
    for a<n {
        print(a);
        a, b = b, a+b;
    }
}

fbi(100);
print();
func factorial(n) {
    if n==0 {return 1;}
    return n*factorial(n-1);
}

print(factorial(5));